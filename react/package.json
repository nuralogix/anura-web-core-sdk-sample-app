{
  "name": "wms",
  "version": "2.0.0",
  "description": "Web Measurement Service",
  "author": "Hamed Hassanzadeh Amin",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "serve:prod": "node --env-file=.prod.env server/index.ts",
    "build": "node --env-file=.prod.env ./node_modules/rollup/dist/bin/rollup --config ./client/config/rollup.config.mjs",
    "serve:dev": "node --env-file=.dev.env server/index.ts",
    "watch": "node --env-file=.dev.env ./node_modules/rollup/dist/bin/rollup --config ./client/config/rollup.config.mjs --watch",
    "lint": "eslint",
    "typecheck:client": "tsc --noEmit --project client/tsconfig.json",
    "typecheck:server": "tsc --noEmit --project server/tsconfig.json",
    "typecheck": "yarn run typecheck:client && yarn run typecheck:server",
    "format:client": "prettier --write 'client/**/*.{ts,tsx}'",
    "format:server": "prettier --write 'server/**/*.{ts,tsx}'"
  },
  "keywords": [
    "WMS",
    "Web Measurement Service"
  ],
  "license": "SEE LICENSE IN LICENSE",
  "devDependencies": {
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/js": "^9.21.0",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-html": "^2.0.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-replace": "^6.0.2",
    "@stylexjs/rollup-plugin": "^0.13.1",
    "@types/compression": "^1.7.5",
    "@types/connect-livereload": "^0.6.3",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/livereload": "^0.9.5",
    "@types/node": "^22.13.5",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/parser": "^8.25.0",
    "@typescript-eslint/types": "^8.25.0",
    "connect-livereload": "^0.6.1",
    "esbuild": "^0.25.0",
    "eslint": "^9.21.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-valtio": "^0.8.0",
    "globals": "^16.0.0",
    "livereload": "^0.9.3",
    "postcss": "^8.5.3",
    "postcss-import": "^16.1.0",
    "prettier": "^3.5.3",
    "rollup": "^4.34.8",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-delete": "^3.0.0",
    "rollup-plugin-esbuild": "^6.2.0",
    "rollup-plugin-inject-process-env": "^1.3.1",
    "rollup-plugin-postcss": "^4.0.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.25.0"
  },
  "dependencies": {
    "@nuralogix.ai/anura-web-core-sdk": "^0.1.0-alpha.21",
    "@nuralogix.ai/dfx-api-client": "^1.0.72",
    "@nuralogix.ai/web-ui": "^0.1.0-alpha.9",
    "@stylexjs/stylex": "0.9.3",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "express": "^5.0.1",
    "i18next": "^24.2.3",
    "i18next-http-backend": "^3.0.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^15.4.1",
    "react-router": "^7.3.0",
    "valtio": "^2.1.3"
  },
  "engines": {
    "node": ">=23"
  }
}
